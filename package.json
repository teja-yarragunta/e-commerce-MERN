{
  "name": "e-commerce",
  "version": "1.0.0",
  "description": "e-commerce application with the MERN stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teja-yarragunta/e-commerce.git"
  },
  "author": "Teja Yarragunta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teja-yarragunta/e-commerce/issues"
  },
  "homepage": "https://github.com/teja-yarragunta/e-commerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "express": "^4.21.2",
    "mongoose": "^8.10.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "nodemon": "^3.1.9"
  }
}
